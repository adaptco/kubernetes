{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "rootDir": "./",
        "outDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}